    "Monorepo",
    "Web",
    "Lint",
    "ESLint",
    "CLI"
  ],
  "main": "./index",
  "typings": "./index.d.ts",
  "author": "Victor Savkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nrwl/nx/issues"
  },
  "homepage": "https://nx.dev",
  "builders": "./executors.json",
  "schematics": "./generators.json",
  "dependencies": {
    "@nx/linter": "file:../../packages/linter"
  },
  "publishConfig": {
    "access": "public"
  },
  "nx-migrations": {
    "migrations": "@nx/linter/migrations.json"
  }
