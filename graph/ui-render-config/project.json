{
  "name": "ui-render-config",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "graph/ui-render-config/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "storybook": {
      "executor": "@nx/storybook:storybook",
      "options": {
        "port": 4400,
        "configDir": "graph/ui-render-config/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nx/storybook:build",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/ui-render-config",
        "configDir": "graph/ui-render-config/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  }
}
