{
  "name": "@nx/vite",
  "version": "0.0.1",
  "private": false,
  "description": "The Nx Plugin for building and testing applications using Vite",
  "repository": {
    "type": "git",
    "url": "https://github.com/nrwl/nx.git",
    "directory": "packages/vite"
  },
  "keywords": [
    "Monorepo",
    "Vite",
    "Web",
    "CLI",
    "Front-end"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "author": "Victor Savkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nrwl/nx/issues"
  },
  "homepage": "https://nx.dev",
  "generators": "./generators.json",
  "executors": "./executors.json",
  "ng-update": {
    "requirements": {},
    "migrations": "./migrations.json"
  },
  "dependencies": {
    "@nx/devkit": "workspace:*",
    "@nx/eslint": "workspace:*",
    "@nx/js": "workspace:*",
    "@nx/vitest": "workspace:*",
    "@phenomnomnominal/tsquery": "catalog:typescript",
    "enquirer": "catalog:",
    "picomatch": "catalog:",
    "tsconfig-paths": "catalog:typescript",
    "semver": "catalog:",
    "tslib": "catalog:typescript",
    "ajv": "^8.0.0"
  },
  "devDependencies": {
    "nx": "workspace:*"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "vitest": "^1.3.1 || ^2.0.0 || ^3.0.0 || ^4.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "@nx/nx-source": "./src/index.ts"
    },
    "./package.json": "./package.json",
    "./migrations.json": "./migrations.json",
    "./generators.json": "./generators.json",
    "./executors.json": "./executors.json",
    "./plugins/*": {
      "import": "./plugins/*.js",
      "types": "./plugins/*.d.ts",
      "@nx/nx-source": "./plugins/*.ts"
    },
    "./src/utils/*": {
      "import": "./dist/utils/*.js",
      "types": "./dist/utils/*.d.ts",
      "@nx/nx-source": "./src/utils/*.ts"
    },
    "./src/executors/*": {
      "import": "./dist/executors/*.js",
      "types": "./dist/executors/*.d.ts",
      "@nx/nx-source": "./src/executors/*.ts"
    },
    "./plugin": {
      "import": "./dist/plugin.js",
      "types": "./dist/plugin.d.ts",
      "@nx/nx-source": "./src/plugin.ts"
    },
    "./executors": "./executors.js",
    "./src/executors/*/schema.json": {
      "import": "./dist/executors/*/schema.json",
      "@nx/nx-source": "./src/executors/*/schema.json"
    },
    "./src/executors/*/*.impl": {
      "import": "./dist/executors/*/*.impl.js",
      "types": "./dist/executors/*/*.impl.d.ts",
      "@nx/nx-source": "./src/executors/*/*.impl.ts"
    },
    "./src/executors/*/compat": {
      "import": "./dist/executors/*/compat.js",
      "types": "./dist/executors/*/compat.d.ts",
      "@nx/nx-source": "./src/executors/*/compat.ts"
    },
    "./plugins/nx-copy-assets.plugin": {
      "import": "./dist/plugins/nx-copy-assets.plugin.js",
      "type": "./dist/plugins/nx-copy-assets.plugin.d.ts",
      "@nx/nx-source": "./src/plugins/nx-copy-assets.plugin.d.ts"
    },
    "./plugins/nx-tsconfig-paths.plugin": {
      "import": "./dist/plugins/nx-tsconfig-paths.plugin.js",
      "type": "./dist/plugins/nx-tsconfig-paths.plugin.d.ts",
      "@nx/nx-source": "./src/plugins/nx-tsconfig-paths.plugin.d.ts"
    },
    "./plugins/rollup-replace-files.plugin": {
      "import": "./dist/plugins/rollup-replace-files.plugin.js",
      "type": "./dist/plugins/rollup-replace-files.plugin.d.ts",
      "@nx/nx-source": "./src/plugins/rollup-replace-files.plugin.d.ts"
    }
  },
  "type": "commonjs"
}
