{
    "name": "@fluffjs/nx",
    "version": "0.0.1",
    "private": true,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "executors": "./executors.json",
    "generators": "./generators.json",
    "nx": {
        "targets": {
            "build": {
                "executor": "nx:run-commands",
                "outputs": [
                    "{projectRoot}/dist"
                ],
                "options": {
                    "commands": [
                        "tsc --build tsconfig.lib.json",
                        "node scripts/copy-assets.mjs",
                        "node scripts/copy-package-json.mjs"
                    ],
                    "cwd": "{projectRoot}",
                    "parallel": false
                }
            }
        }
    },
    "dependencies": {
        "@fluffjs/cli": "0.0.3",
        "@nx/devkit": "^21.0.0",
        "tslib": "^2.3.0"
    },
    "peerDependencies": {
        "nx": ">=19.0.0"
    }
}
