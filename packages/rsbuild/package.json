{
  "name": "@nx/rsbuild",
  "description": "The Nx Plugin for Rsbuild contains an Nx plugin, executors and utilities that support building applications using Rsbuild.",
  "version": "0.0.1",
  "type": "commonjs",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nrwl/nx.git",
    "directory": "packages/rsbuild"
  },
  "bugs": {
    "url": "https://github.com/nrwl/nx/issues"
  },
  "keywords": [
    "Monorepo",
    "Rspack",
    "Webpack",
    "Bundling",
    "Module Federation",
    "Rsbuild"
  ],
  "author": "Colum Ferry",
  "license": "MIT",
  "homepage": "https://nx.dev",
  "main": "index.js",
  "generators": "./generators.json",
  "executors": "./executors.json",
  "dependencies": {
    "tslib": "catalog:typescript",
    "@nx/devkit": "workspace:*",
    "@nx/js": "workspace:*",
    "@rsbuild/core": "1.1.8",
    "minimatch": "catalog:",
    "@phenomnomnominal/tsquery": "catalog:typescript"
  },
  "devDependencies": {
    "nx": "workspace:*"
  },
  "peerDependencies": {},
  "nx-migrations": {
    "migrations": "./migrations.json"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "@nx/nx-source": "./src/index.ts"
    },
    "./package.json": "./package.json",
    "./migrations.json": "./migrations.json",
    "./generators.json": "./generators.json",
    "./executors.json": "./executors.json",
    "./src/utils/*": {
      "import": "./dist/utils/*.js",
      "types": "./dist/utils/*.d.ts",
      "@nx/nx-source": "./src/utils/*.ts"
    },
    "./src/generators/*": {
      "import": "./dist/generators/*.js",
      "types": "./dist/generators/*.d.ts",
      "@nx/nx-source": "./src/generators/*.ts"
    },
    "./src/executors/*": {
      "import": "./dist/executors/*.js",
      "types": "./dist/executors/*.d.ts",
      "@nx/nx-source": "./src/executors/*.ts"
    },
    "./plugins/*": {
      "import": "./plugins/*.js",
      "types": "./plugins/*.d.ts",
      "@nx/nx-source": "./plugins/*.ts"
    }
  }
}
