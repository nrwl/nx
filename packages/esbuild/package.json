{
  "name": "@nx/esbuild",
  "version": "0.0.1",
  "private": false,
  "type": "commonjs",
  "description": "The Nx Plugin for esbuild contains executors and generators that support building applications using esbuild",
  "repository": {
    "type": "git",
    "url": "https://github.com/nrwl/nx.git",
    "directory": "packages/esbuild"
  },
  "keywords": [
    "Monorepo",
    "EsBuild",
    "Web",
    "CLI",
    "Front-end",
    "Backend"
  ],
  "main": "./dist/index.js",
  "typings": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "@nx/nx-source": "./index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./executors.json": "./executors.json",
    "./generators.json": "./generators.json",
    "./migrations.json": "./migrations.json",
    "./src/executors/*": {
      "types": "./src/executors/*.d.ts",
      "@nx/nx-source": "./src/executors/*.ts",
      "import": "./dist/src/executors/*.js",
      "default": "./dist/src/executors/*.js"
    },
    "./src/executors/**/*": {
      "types": "./src/executors/**/*.d.ts",
      "@nx/nx-source": "./src/executors/**/*.ts",
      "import": "./dist/src/executors/**/*.js",
      "default": "./dist/src/executors/**/*.js"
    },
    "./src/generators/*": {
      "types": "./src/generators/*.d.ts",
      "@nx/nx-source": "./src/generators/*.ts",
      "import": "./dist/src/generators/*.js",
      "default": "./dist/src/generators/*.js"
    },
    "./src/generators/**/*": {
      "types": "./src/generators/**/*.d.ts",
      "@nx/nx-source": "./src/generators/**/*.ts",
      "import": "./dist/src/generators/**/*.js",
      "default": "./dist/src/generators/**/*.js"
    },
    "./src/utils/*": {
      "types": "./src/utils/*.d.ts",
      "@nx/nx-source": "./src/utils/*.ts",
      "import": "./dist/src/utils/*.js",
      "default": "./dist/src/utils/*.js"
    }
  },
  "author": "Victor Savkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nrwl/nx/issues"
  },
  "homepage": "https://nx.dev",
  "generators": "./generators.json",
  "executors": "./executors.json",
  "ng-update": {
    "requirements": {},
    "migrations": "./migrations.json"
  },
  "dependencies": {
    "@nx/devkit": "workspace:*",
    "@nx/js": "workspace:*",
    "tinyglobby": "catalog:",
    "picocolors": "catalog:",
    "tsconfig-paths": "catalog:typescript",
    "tslib": "catalog:typescript"
  },
  "devDependencies": {
    "nx": "workspace:*"
  },
  "peerDependencies": {
    "esbuild": ">=0.19.2 <1.0.0"
  },
  "peerDependenciesMeta": {
    "esbuild": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
