{
  "name": "@nx/rspack",
  "description": "The Nx Plugin for Rspack contains executors and generators that support building applications using Rspack.",
  "version": "0.0.1",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/nrwl/nx.git",
    "directory": "packages/rspack"
  },
  "bugs": {
    "url": "https://github.com/nrwl/nx/issues"
  },
  "keywords": [
    "Monorepo",
    "Rspack",
    "Bundling",
    "Module Federation"
  ],
  "author": "Jack Hsu",
  "license": "MIT",
  "homepage": "https://nx.dev",
  "main": "./dist/src/index.js",
  "typings": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "@nx/nx-source": "./src/index.ts",
      "import": "./dist/src/index.js",
      "default": "./dist/src/index.js"
    },
    "./plugin": {
      "types": "./plugin.d.ts",
      "@nx/nx-source": "./plugin.ts",
      "import": "./dist/plugin.js",
      "default": "./dist/plugin.js"
    },
    "./module-federation": {
      "types": "./module-federation.d.ts",
      "@nx/nx-source": "./module-federation.ts",
      "import": "./dist/module-federation.js",
      "default": "./dist/module-federation.js"
    },
    "./react-plugin": {
      "types": "./react-plugin.d.ts",
      "@nx/nx-source": "./react-plugin.ts",
      "import": "./dist/react-plugin.js",
      "default": "./dist/react-plugin.js"
    },
    "./app-plugin": {
      "types": "./app-plugin.d.ts",
      "@nx/nx-source": "./app-plugin.ts",
      "import": "./dist/app-plugin.js",
      "default": "./dist/app-plugin.js"
    },
    "./package.json": "./package.json",
    "./executors.json": "./executors.json",
    "./generators.json": "./generators.json",
    "./migrations.json": "./migrations.json",
    "./src/executors/*": {
      "types": "./src/executors/*.d.ts",
      "@nx/nx-source": "./src/executors/*.ts",
      "import": "./dist/src/executors/*.js",
      "default": "./dist/src/executors/*.js"
    },
    "./src/executors/**/*": {
      "types": "./src/executors/**/*.d.ts",
      "@nx/nx-source": "./src/executors/**/*.ts",
      "import": "./dist/src/executors/**/*.js",
      "default": "./dist/src/executors/**/*.js"
    },
    "./src/generators/*": {
      "types": "./src/generators/*.d.ts",
      "@nx/nx-source": "./src/generators/*.ts",
      "import": "./dist/src/generators/*.js",
      "default": "./dist/src/generators/*.js"
    },
    "./src/generators/**/*": {
      "types": "./src/generators/**/*.d.ts",
      "@nx/nx-source": "./src/generators/**/*.ts",
      "import": "./dist/src/generators/**/*.js",
      "default": "./dist/src/generators/**/*.js"
    },
    "./src/plugins/*": {
      "types": "./src/plugins/*.d.ts",
      "@nx/nx-source": "./src/plugins/*.ts",
      "import": "./dist/src/plugins/*.js",
      "default": "./dist/src/plugins/*.js"
    },
    "./src/plugins/**/*": {
      "types": "./src/plugins/**/*.d.ts",
      "@nx/nx-source": "./src/plugins/**/*.ts",
      "import": "./dist/src/plugins/**/*.js",
      "default": "./dist/src/plugins/**/*.js"
    },
    "./src/utils/*": {
      "types": "./src/utils/*.d.ts",
      "@nx/nx-source": "./src/utils/*.ts",
      "import": "./dist/src/utils/*.js",
      "default": "./dist/src/utils/*.js"
    },
    "./src/utils/**/*": {
      "types": "./src/utils/**/*.d.ts",
      "@nx/nx-source": "./src/utils/**/*.ts",
      "import": "./dist/src/utils/**/*.js",
      "default": "./dist/src/utils/**/*.js"
    }
  },
  "generators": "./generators.json",
  "executors": "./executors.json",
  "dependencies": {
    "@nx/devkit": "workspace:*",
    "@nx/js": "workspace:*",
    "@nx/module-federation": "workspace:*",
    "@nx/web": "workspace:*",
    "@phenomnomnominal/tsquery": "catalog:typescript",
    "@rspack/core": "catalog:rspack",
    "@rspack/dev-server": "catalog:rspack",
    "@rspack/plugin-react-refresh": "catalog:rspack",
    "autoprefixer": "^10.4.9",
    "browserslist": "^4.26.0",
    "css-loader": "^6.4.0",
    "enquirer": "catalog:",
    "express": "^4.21.2",
    "http-proxy-middleware": "^3.0.5",
    "less-loader": "^11.1.0",
    "license-webpack-plugin": "^4.0.2",
    "loader-utils": "^2.0.3",
    "parse5": "4.0.0",
    "picocolors": "catalog:",
    "postcss": "^8.4.38",
    "postcss-import": "~14.1.0",
    "postcss-loader": "^8.1.1",
    "sass": "^1.85.0",
    "sass-embedded": "^1.83.4",
    "sass-loader": "^16.0.4",
    "source-map-loader": "^5.0.0",
    "style-loader": "^3.3.0",
    "ts-checker-rspack-plugin": "catalog:rspack",
    "tslib": "catalog:typescript",
    "webpack": "^5.101.3",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "nx": "workspace:*",
    "@nx/nest": "workspace:*"
  },
  "peerDependencies": {
    "@module-federation/enhanced": "^0.21.2",
    "@module-federation/node": "^2.7.21"
  },
  "nx-migrations": {
    "migrations": "./migrations.json"
  },
  "publishConfig": {
    "access": "public"
  }
}
