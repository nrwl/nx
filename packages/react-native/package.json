{
  "name": "@nx/react-native",
  "version": "0.0.1",
  "private": false,
  "type": "commonjs",
  "description": "The Nx Plugin for React Native contains generators for managing React Native applications and libraries within an Nx workspace. It provides: \n\n-Integration with libraries such as Jest, Detox, and Storybook.\n-Scaffolding for creating buildable libraries that can be published to npm.\n-Utilities for automatic workspace refactoring.",
  "keywords": [
    "Monorepo",
    "React",
    "Web",
    "Jest",
    "Native",
    "CLI",
    "Mobile"
  ],
  "homepage": "https://nx.dev",
  "bugs": {
    "url": "https://github.com/nrwl/nx/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nrwl/nx.git",
    "directory": "packages/react-native"
  },
  "license": "MIT",
  "author": "Victor Savkin",
  "main": "./dist/index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./index.d.ts",
      "@nx/nx-source": "./index.ts"
    },
    "./package.json": "./package.json",
    "./generators.json": "./generators.json",
    "./executors.json": "./executors.json",
    "./migrations.json": "./migrations.json",
    "./plugin": {
      "import": "./dist/plugin.js",
      "types": "./plugin.d.ts",
      "@nx/nx-source": "./plugins.ts"
    },
    "./plugins/*": {
      "import": "./dist/plugins/*.js",
      "types": "./plugins/*.d.ts",
      "@nx/nx-source": "./plugins/*.ts"
    },
    "./typings/*": {
      "types": "./typings/*.d.ts",
      "@nx/nx-source": "./typings/*.ts"
    },
    "./plugins/**/*": {
      "import": "./dist/plugins/**/*.js",
      "types": "./plugins/**/*.d.ts",
      "@nx/nx-source": "./plugins/**/*.ts"
    },
    "./src/generators/*": {
      "import": "./dist/src/generators/*.js",
      "types": "./src/generators/*.d.ts",
      "@nx/nx-source": "./src/generators/*.ts"
    },
    "./src/generators/**/*": {
      "import": "./dist/src/generators/**/*.js",
      "types": "./src/generators/**/*.d.ts",
      "@nx/nx-source": "./src/generators/**/*.ts"
    },
    "./src/executors/*": {
      "import": "./dist/src/executors/*.js",
      "types": "./src/executors/*.d.ts",
      "@nx/nx-source": "./src/executors/*.ts"
    },
    "./src/executors/**/*": {
      "types": "./src/executors/**/*.d.ts",
      "import": "./dist/src/executors/**/*.js",
      "@nx/nx-source": "./src/executors/**/*.ts"
    },
    "./src/utils/*": {
      "types": "./src/utils/*.d.ts",
      "import": "./dist/src/utils/*.js",
      "@nx/nx-source": "./src/utils/*.ts"
    },
    "./src/utils/**/*": {
      "types": "./src/utils/**/*.d.ts",
      "import": "./dist/src/utils/**/*.js",
      "@nx/nx-source": "./src/utils/**/*.ts"
    }
  },
  "dependencies": {
    "@nx/devkit": "workspace:*",
    "@nx/js": "workspace:*",
    "@nx/eslint": "workspace:*",
    "@nx/react": "workspace:*",
    "ajv": "^8.12.0",
    "enhanced-resolve": "^5.8.3",
    "ignore": "^5.0.4",
    "picocolors": "catalog:",
    "tinyglobby": "catalog:",
    "tsconfig-paths": "catalog:typescript",
    "tslib": "catalog:typescript"
  },
  "peerDependencies": {
    "metro-config": ">= 0.82.0",
    "metro-resolver": ">= 0.82.0"
  },
  "optionalDependencies": {
    "@nx/detox": "workspace:*",
    "@nx/rollup": "workspace:*"
  },
  "executors": "./executors.json",
  "ng-update": {
    "requirements": {},
    "migrations": "./migrations.json"
  },
  "generators": "./generators.json",
  "publishConfig": {
    "access": "public"
  }
}
