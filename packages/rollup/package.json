{
  "name": "@nx/rollup",
  "version": "0.0.1",
  "private": false,
  "type": "commonjs",
  "description": "The Nx Plugin for Rollup contains executors and generators that support building applications using Rollup.",
  "repository": {
    "type": "git",
    "url": "https://github.com/nrwl/nx.git",
    "directory": "packages/rollup"
  },
  "keywords": [
    "Monorepo",
    "Rollup",
    "Web",
    "CLI"
  ],
  "main": "./dist/index.js",
  "typings": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "@nx/nx-source": "./index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./plugin": {
      "types": "./plugin.d.ts",
      "@nx/nx-source": "./plugin.ts",
      "import": "./dist/plugin.js",
      "default": "./dist/plugin.js"
    },
    "./with-nx": {
      "types": "./with-nx.d.ts",
      "@nx/nx-source": "./with-nx.ts",
      "import": "./dist/with-nx.js",
      "default": "./dist/with-nx.js"
    },
    "./package.json": "./package.json",
    "./generators.json": "./generators.json",
    "./executors.json": "./executors.json",
    "./migrations.json": "./migrations.json",
    "./src/generators/*": {
      "types": "./src/generators/*.d.ts",
      "@nx/nx-source": "./src/generators/*.ts",
      "import": "./dist/src/generators/*.js",
      "default": "./dist/src/generators/*.js"
    },
    "./src/generators/**/*": {
      "types": "./src/generators/**/*.d.ts",
      "@nx/nx-source": "./src/generators/**/*.ts",
      "import": "./dist/src/generators/**/*.js",
      "default": "./dist/src/generators/**/*.js"
    },
    "./src/executors/*": {
      "types": "./src/executors/*.d.ts",
      "@nx/nx-source": "./src/executors/*.ts",
      "import": "./dist/src/executors/*.js",
      "default": "./dist/src/executors/*.js"
    },
    "./src/executors/**/*": {
      "types": "./src/executors/**/*.d.ts",
      "@nx/nx-source": "./src/executors/**/*.ts",
      "import": "./dist/src/executors/**/*.js",
      "default": "./dist/src/executors/**/*.js"
    },
    "./src/utils/*": {
      "types": "./src/utils/*.d.ts",
      "@nx/nx-source": "./src/utils/*.ts",
      "import": "./dist/src/utils/*.js",
      "default": "./dist/src/utils/*.js"
    },
    "./src/utils/**/*": {
      "types": "./src/utils/**/*.d.ts",
      "@nx/nx-source": "./src/utils/**/*.ts",
      "import": "./dist/src/utils/**/*.js",
      "default": "./dist/src/utils/**/*.js"
    },
    "./src/plugins/*": {
      "types": "./src/plugins/*.d.ts",
      "@nx/nx-source": "./src/plugins/*.ts",
      "import": "./dist/src/plugins/*.js",
      "default": "./dist/src/plugins/*.js"
    },
    "./src/plugins/**/*": {
      "types": "./src/plugins/**/*.d.ts",
      "@nx/nx-source": "./src/plugins/**/*.ts",
      "import": "./dist/src/plugins/**/*.js",
      "default": "./dist/src/plugins/**/*.js"
    }
  },
  "author": "Victor Savkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nrwl/nx/issues"
  },
  "homepage": "https://nx.dev",
  "generators": "./generators.json",
  "executors": "./executors.json",
  "ng-update": {
    "requirements": {},
    "migrations": "./migrations.json"
  },
  "dependencies": {
    "@nx/devkit": "workspace:*",
    "@nx/js": "workspace:*",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^12.1.0",
    "autoprefixer": "^10.4.9",
    "picocolors": "catalog:",
    "picomatch": "catalog:",
    "postcss": "^8.4.38",
    "rollup": "^4.14.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "catalog:typescript"
  },
  "devDependencies": {
    "jest-environment-node": "catalog:jest",
    "nx": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
