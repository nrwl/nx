{
  "extends": "../../.eslintrc",
  "rules": {
    "no-restricted-imports": [
      "error",
      "@nx/workspace",
      "@angular-devkit/core",
      "@angular-devkit/architect",
      "@angular-devkit/schematics",
      "nx",
      "@nx/devkit"
    ]
  },
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
