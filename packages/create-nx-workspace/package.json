{
  "name": "create-nx-workspace",
  "version": "0.0.1",
  "private": false,
  "type": "commonjs",
  "description": "Smart Repos Â· Fast Builds",
  "repository": {
    "type": "git",
    "url": "https://github.com/nrwl/nx.git",
    "directory": "packages/create-nx-workspace"
  },
  "keywords": [
    "Monorepo",
    "Angular",
    "React",
    "Web",
    "Node",
    "Nest",
    "Jest",
    "Cypress",
    "Playwright",
    "CLI"
  ],
  "main": "./dist/index.js",
  "typings": "./index.d.ts",
  "bin": {
    "create-nx-workspace": "./dist/bin/index.js"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "@nx/nx-source": "./index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./bin/create-nx-workspace": {
      "types": "./bin/create-nx-workspace.d.ts",
      "@nx/nx-source": "./bin/create-nx-workspace.ts",
      "import": "./dist/bin/create-nx-workspace.js",
      "default": "./dist/bin/create-nx-workspace.js"
    },
    "./src/*": {
      "types": "./src/*.d.ts",
      "@nx/nx-source": "./src/*.ts",
      "import": "./dist/src/*.js",
      "default": "./dist/src/*.js"
    },
    "./src/**/*": {
      "types": "./src/**/*.d.ts",
      "@nx/nx-source": "./src/**/*.ts",
      "import": "./dist/src/**/*.js",
      "default": "./dist/src/**/*.js"
    },
    "./package.json": "./package.json"
  },
  "author": "Victor Savkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nrwl/nx/issues"
  },
  "homepage": "https://nx.dev",
  "dependencies": {
    "axios": "^1.12.0",
    "chalk": "catalog:",
    "enquirer": "catalog:",
    "flat": "^5.0.2",
    "ora": "5.3.0",
    "tmp": "~0.2.1",
    "tslib": "catalog:typescript",
    "yargs": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
