{
  "name": "@nx/plugin",
  "version": "0.0.1",
  "private": false,
  "description": "This plugin is used to create Nx plugins! It contains generators for generating common plugin features like generators, executors, migrations and more.",
  "repository": {
    "type": "git",
    "url": "https://github.com/nrwl/nx.git",
    "directory": "packages/plugin"
  },
  "keywords": [
    "Monorepo",
    "Nx",
    "CLI"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Nrwl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nrwl/nx/issues"
  },
  "homepage": "https://nx.dev",
  "generators": "./generators.json",
  "ng-update": {
    "requirements": {},
    "migrations": "./migrations.json"
  },
  "dependencies": {
    "tslib": "catalog:typescript",
    "@nx/devkit": "workspace:*",
    "@nx/jest": "workspace:*",
    "@nx/js": "workspace:*",
    "@nx/eslint": "workspace:*"
  },
  "devDependencies": {
    "nx": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "@nx/nx-source": "./src/index.ts"
    },
    "./package.json": "./package.json",
    "./migrations.json": "./migrations.json",
    "./generators.json": "./generators.json",
    "./src/utils/*": {
      "import": "./dist/utils/*.js",
      "types": "./dist/utils/*.d.ts",
      "@nx/nx-source": "./src/utils/*.ts"
    },
    "./src/generators/*": {
      "import": "./dist/generators/*.js",
      "types": "./dist/generators/*.d.ts",
      "@nx/nx-source": "./src/generators/*.ts"
    },
    "./generators": {
      "import": "./dist/generators.js",
      "types": "./dist/generators.d.ts",
      "@nx/nx-source": "./src/generators.ts"
    },
    "./src/executors/*": {
      "import": "./dist/executors/*.js",
      "types": "./dist/executors/*.d.ts",
      "@nx/nx-source": "./src/executors/*.ts"
    },
    "./testing": {
      "import": "./dist/testing.js",
      "types": "./dist/testing.d.ts",
      "@nx/nx-source": "./src/generators.ts"
    },
    "./plugins/*": {
      "import": "./plugins/*.js",
      "types": "./plugins/*.d.ts",
      "@nx/nx-source": "./plugins/*.ts"
    }
  }
}
