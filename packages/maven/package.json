{
  "name": "@nx/maven",
  "version": "0.0.3",
  "description": "Nx plugin for Maven integration",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "generators": "./generators.json",
  "executors": "./executors.json",
  "keywords": [
    "nx",
    "maven",
    "plugin",
    "java",
    "build"
  ],
  "author": "Nx Team",
  "license": "MIT",
  "scripts": {
    "test:unit": "nx test --testPathIgnorePatterns=\".*\\.e2e\\.spec\\.ts\"",
    "test:e2e": "nx test --testPathPattern=\".*\\.e2e\\.spec\\.ts\" --testTimeout=400000",
    "test:maven-cli": "nx test --testPathPattern=\".*\\.e2e\\.spec\\.ts\" --testNamePattern=\"maven-cli install\" --testTimeout=400000"
  },
  "dependencies": {
    "@nx/devkit": "^21.4.0",
    "@types/xmldom": "^0.1.34",
    "tslib": "^2.3.0",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^20.19.10",
    "jest": "^29.4.0",
    "memfs": "^4.9.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
