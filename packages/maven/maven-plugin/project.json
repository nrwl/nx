{
  "name": "dev.nx.maven:nx-maven-plugin",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/maven/maven-plugin",
  "projectType": "library",
  "targets": {
    "install": {
      "command": "for i in {1..5}; do ./mvnw install -pl dev.nx.maven:nx-maven-plugin -am && exit 0; sleep 10; done; exit 1"
    },
    "_install": {
      "command": "node scripts/run-native-target.js install nx-maven-plugin"
    }
  },
  "tags": ["maven:dev.nx.maven"],
  "implicitDependencies": ["dev.nx.maven:nx-maven-shared"]
}
