{
  "name": "Nx Cypress",
  "version": "0.1",
  "schematics": {
    "init": {
      "factory": "./src/generators/init/init#cypressInitSchematic",
      "schema": "./src/generators/init/schema.json",
      "description": "Initialize the `@nrwl/cypress` plugin.",
      "aliases": ["ng-add"],
      "hidden": true
    },
    "cypress-project": {
      "factory": "./src/generators/cypress-project/cypress-project#cypressProjectSchematic",
      "schema": "./src/generators/cypress-project/schema.json",
      "description": "Add a Cypress E2E Project"
    },
    "cypress-component-project": {
      "factory": "./src/generators/cypress-component-project/cypress-component-project#cypressComponentProject",
      "schema": "./src/generators/cypress-component-project/schema.json",
      "aliases": ["cy-cmp"],
      "description": "Set up Cypress Component Test for a project"
    },
    "cypress-component-test-file": {
      "factory": "./src/generators/cypress-component-test-file/cypress-component-test-file#cypressComponentTestFiles",
      "schema": "./src/generators/cypress-component-test-file/schema.json",
      "aliases": ["cy-test"],
      "description": "Create a Cypress component test for a given component"
    },
    "convert-cypress-ten": {
      "factory": "./src/generators/convert-to-cypress-10/convert-to-cypress-ten#convertCypressProject",
      "schema": "./src/generators/convert-to-cypress-10/schema.json",
      "aliases": ["cy10"],
      "description": "Convert existing Cypress e2e projects to Cypress 10"
    }
  },
  "generators": {
    "init": {
      "factory": "./src/generators/init/init#cypressInitGenerator",
      "schema": "./src/generators/init/schema.json",
      "description": "Initialize the `@nrwl/cypress` plugin.",
      "aliases": ["ng-add"],
      "hidden": true
    },
    "cypress-project": {
      "factory": "./src/generators/cypress-project/cypress-project#cypressProjectGenerator",
      "schema": "./src/generators/cypress-project/schema.json",
      "description": "Add a Cypress E2E Project.",
      "hidden": true
    },
    "cypress-component-project": {
      "factory": "./src/generators/cypress-component-project/cypress-component-project#cypressComponentProject",
      "schema": "./src/generators/cypress-component-project/schema.json",
      "description": "Set up Cypress Component Test for a project",
      "aliases": ["cy-cmp"],
      "hidden": true
    },
    "convert-cypress-ten": {
      "factory": "./src/generators/convert-to-cypress-10/convert-to-cypress-ten#convertCypressProject",
      "schema": "./src/generators/convert-to-cypress-10/schema.json",
      "aliases": ["cy10"],
      "description": "Convert to Cypress 10",
      "hidden": true
    },
    "cypress-component-test-file": {
      "factory": "./src/generators/cypress-component-test-file/cypress-component-test-file#cypressComponentTestFiles",
      "schema": "./src/generators/cypress-component-test-file/schema.json",
      "alias": ["cy-test"],
      "description": "Create a Cypress component test for a given component",
      "hidden": true
    }
  }
}
