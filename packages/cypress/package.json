{
  "name": "@nx/cypress",
  "version": "0.0.1",
  "private": false,
  "type": "commonjs",
  "description": "The Nx Plugin for Cypress contains executors and generators allowing your workspace to use the powerful Cypress integration testing capabilities.",
  "repository": {
    "type": "git",
    "url": "https://github.com/nrwl/nx.git",
    "directory": "packages/cypress"
  },
  "keywords": [
    "Monorepo",
    "Angular",
    "React",
    "Web",
    "Node",
    "Nest",
    "Jest",
    "Cypress",
    "CLI",
    "Front-end",
    "Testing"
  ],
  "main": "./dist/index.js",
  "typings": "./index.d.ts",
  "author": "Victor Savkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nrwl/nx/issues"
  },
  "homepage": "https://nx.dev",
  "generators": "./generators.json",
  "executors": "./executors.json",
  "ng-update": {
    "requirements": {},
    "migrations": "./migrations.json"
  },
  "dependencies": {
    "@nx/devkit": "workspace:*",
    "@nx/eslint": "workspace:*",
    "@nx/js": "workspace:*",
    "@phenomnomnominal/tsquery": "catalog:typescript",
    "detect-port": "^1.5.1",
    "semver": "catalog:",
    "tree-kill": "1.2.2",
    "tslib": "catalog:typescript"
  },
  "devDependencies": {
    "nx": "workspace:*"
  },
  "peerDependencies": {
    "cypress": ">= 13 < 16"
  },
  "peerDependenciesMeta": {
    "cypress": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./index.d.ts",
      "@nx/nx-source": "./index.ts"
    },
    "./package.json": "./package.json",
    "./migrations.json": "./migrations.json",
    "./generators.json": "./generators.json",
    "./executors.json": "./executors.json",
    "./plugin": {
      "import": "./dist/plugin.js",
      "types": "./plugin.d.ts",
      "@nx/nx-source": "./plugin.ts"
    },
    "./plugins/*": {
      "import": "./dist/plugins/*.js",
      "types": "./plugins/*.d.ts",
      "@nx/nx-source": "./plugins/*.ts"
    },
    "./plugins/cypress-preset": {
      "import": "./dist/plugins/cypress-preset.js",
      "types": "./plugins/cypress-preset.d.ts",
      "@nx/nx-source": "./plugins/cypress-preset.ts"
    },
    "./src/generators/*": {
      "import": "./dist/src/generators/*.js",
      "types": "./src/generators/*.d.ts",
      "@nx/nx-source": "./src/generators/*.ts"
    },
    "./src/generators/**/*": {
      "import": "./dist/src/generators/**/*.js",
      "types": "./src/generators/**/*.d.ts",
      "@nx/nx-source": "./src/generators/**/*.ts"
    },
    "./src/executors/*": {
      "import": "./dist/src/executors/*.js",
      "types": "./src/executors/*.d.ts",
      "@nx/nx-source": "./src/executors/*.ts"
    },
    "./src/executors/**/*": {
      "import": "./dist/src/executors/**/*.js",
      "types": "./src/executors/**/*.d.ts",
      "@nx/nx-source": "./src/executors/**/*.ts"
    },
    "./src/plugins/*": {
      "import": "./dist/src/plugins/*.js",
      "types": "./src/plugins/*.d.ts",
      "@nx/nx-source": "./src/plugins/*.ts"
    },
    "./src/utils/*": {
      "import": "./dist/src/utils/*.js",
      "types": "./src/utils/*.d.ts",
      "@nx/nx-source": "./src/utils/*.ts"
    },
    "./generators/*/schema.json": "./dist/src/generators/*/schema.json",
    "./generators/*/schema": "./src/generators/*/schema.d.ts",
    "./executors/*/schema.json": "./dist/src/executors/*/schema.json",
    "./executors/*/schema": "./src/executors/*/schema.d.ts"
  },
  "publishConfig": {
    "access": "public"
  }
}
