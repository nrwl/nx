{
  "name": "@nx/expo",
  "version": "0.0.1",
  "private": false,
  "type": "commonjs",
  "description": "The Expo Plugin for Nx contains executors and generators for managing and developing an expo application within your workspace. For example, you can directly build for different target platforms as well as generate projects and publish your code.",
  "keywords": [
    "Monorepo",
    "Expo",
    "React",
    "Web",
    "Jest",
    "Native",
    "CLI",
    "Mobile",
    "Testing"
  ],
  "homepage": "https://nx.dev",
  "bugs": {
    "url": "https://github.com/nrwl/nx/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nrwl/nx.git",
    "directory": "packages/expo"
  },
  "license": "MIT",
  "author": "Victor Savkin",
  "main": "./dist/index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./index.d.ts",
      "@nx/nx-source": "./index.ts"
    },
    "./plugin": {
      "import": "./dist/plugin.js",
      "types": "./plugin.d.ts",
      "@nx/nx-source": "./plugin.ts"
    },
    "./package.json": "./package.json",
    "./migrations.json": "./migrations.json",
    "./generators.json": "./generators.json",
    "./executors.json": "./executors.json",
    "./src/generators/*": {
      "import": "./dist/src/generators/*.js",
      "types": "./src/generators/*.d.ts",
      "@nx/nx-source": "./src/generators/*.ts"
    },
    "./src/generators/**/*": {
      "import": "./dist/src/generators/**/*.js",
      "types": "./src/generators/**/*.d.ts",
      "@nx/nx-source": "./src/generators/**/*.ts"
    },
    "./src/executors/*": {
      "import": "./dist/src/executors/*.js",
      "types": "./src/executors/*.d.ts",
      "@nx/nx-source": "./src/executors/*.ts"
    },
    "./src/executors/**/*": {
      "import": "./dist/src/executors/**/*.js",
      "types": "./src/executors/**/*.d.ts",
      "@nx/nx-source": "./src/executors/**/*.ts"
    },
    "./plugins/*": {
      "import": "./dist/plugins/*.js",
      "types": "./plugins/*.d.ts",
      "@nx/nx-source": "./plugins/*.ts"
    },
    "./plugins/**/*": {
      "import": "./dist/plugins/**/*.js",
      "types": "./plugins/**/*.d.ts",
      "@nx/nx-source": "./plugins/**/*.ts"
    },
    "./src/utils/*": {
      "import": "./dist/src/utils/*.js",
      "types": "./src/utils/*.d.ts",
      "@nx/nx-source": "./src/utils/*.ts"
    },
    "./src/utils/**/*": {
      "import": "./dist/src/utils/**/*.js",
      "types": "./src/utils/**/*.d.ts",
      "@nx/nx-source": "./src/utils/**/*.ts"
    }
  },
  "dependencies": {
    "@nx/devkit": "workspace:*",
    "@nx/eslint": "workspace:*",
    "@nx/js": "workspace:*",
    "@nx/react": "workspace:*",
    "enhanced-resolve": "^5.8.3",
    "picocolors": "catalog:",
    "semver": "catalog:",
    "tsconfig-paths": "catalog:typescript",
    "tslib": "catalog:typescript"
  },
  "peerDependencies": {
    "metro-config": ">= 0.82.0",
    "metro-resolver": ">= 0.82.0"
  },
  "optionalDependencies": {
    "@nx/detox": "workspace:*",
    "@nx/rollup": "workspace:*"
  },
  "executors": "./executors.json",
  "ng-update": {
    "requirements": {},
    "migrations": "./migrations.json"
  },
  "generators": "./generators.json",
  "publishConfig": {
    "access": "public"
  }
}
