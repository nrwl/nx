{
    "name": "@fluffjs/cli",
    "version": "0.0.3",
    "private": true,
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": {
        "fluff": "./dist/bin.js"
    },
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "@fluffjs/source": "./src/index.ts",
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "!**/*.tsbuildinfo"
    ],
    "dependencies": {
        "@babel/core": "^7.28.6",
        "@babel/generator": "^7.28.6",
        "@babel/parser": "^7.28.6",
        "@babel/plugin-syntax-decorators": "^7.28.6",
        "@babel/preset-typescript": "^7.28.5",
        "@babel/traverse": "^7.28.6",
        "@babel/types": "^7.28.6",
        "esbuild": "^0.27.2",
        "he": "^1.2.0",
        "html-minifier-terser": "^7.2.0",
        "http-proxy": "^1.18.1",
        "picomatch": "^4.0.2",
        "parse5": "^8.0.0",
        "parse5-sax-parser": "^8.0.0",
        "source-map": "^0.7.6",
        "tslib": "^2.3.0"
    },
    "nx": {
        "targets": {
            "build": {
                "executor": "nx:run-commands",
                "outputs": [
                    "{projectRoot}/dist"
                ],
                "options": {
                    "commands": [
                        "tsc --build tsconfig.lib.json",
                        "node scripts/copy-package-json.mjs"
                    ],
                    "cwd": "{projectRoot}",
                    "parallel": false
                }
            },
            "test": {
                "executor": "@nx/vitest:test",
                "outputs": [
                    "{options.reportsDirectory}"
                ],
                "options": {
                    "reportsDirectory": "../../coverage/packages/cli"
                }
            }
        }
    },
    "devDependencies": {
        "@types/he": "^1.2.3",
        "@types/http-proxy": "^1.17.16",
        "@types/jsesc": "^3.0.3",
        "jsesc": "^3.1.0"
    }
}
