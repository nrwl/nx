{
  "name": "@nx/devkit",
  "version": "0.0.1",
  "private": false,
  "description": "The Nx Devkit is used to customize Nx for different technologies and use cases. It contains many utility functions for reading and writing files, updating configuration, working with Abstract Syntax Trees(ASTs), and more. Learn more about [extending Nx by leveraging the Nx Devkit](https://nx.dev/extending-nx/intro/getting-started) on our docs.",
  "repository": {
    "type": "git",
    "url": "https://github.com/nrwl/nx.git",
    "directory": "packages/devkit"
  },
  "keywords": [
    "Monorepo",
    "Angular",
    "React",
    "Web",
    "Node",
    "Nest",
    "Jest",
    "Cypress",
    "CLI"
  ],
  "main": "./index.js",
  "typings": "./index.d.ts",
  "author": "Victor Savkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nrwl/nx/issues"
  },
  "homepage": "https://nx.dev",
  "dependencies": {
    "@zkochan/js-yaml": "catalog:",
    "ejs": "^3.1.7",
    "tslib": "catalog:typescript",
    "semver": "catalog:",
    "yargs-parser": "catalog:",
    "minimatch": "catalog:",
    "enquirer": "catalog:"
  },
  "devDependencies": {
    "jest": "catalog:jest",
    "nx": "workspace:*"
  },
  "peerDependencies": {
    "nx": ">= 21 <= 23 || ^22.0.0-0"
  },
  "publishConfig": {
    "access": "public"
  },
  "nx-migrations": {
    "migrations": "./migrations.json"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "@nx/nx-source": "./src/index.ts"
    },
    "./package.json": "./package.json",
    "./migrations.json": "./migrations.json",
    "./testing": {
      "import": "./dist/testing.js",
      "types": "./dist/testing.d.ts",
      "@nx/nx-source": "./testing.ts"
    },
    "./internal": {
      "import": "./dist/internal.js",
      "types": "./dist/internal.d.ts",
      "@nx/nx-source": "./internal.ts"
    },
    "./internal-testing-utils": {
      "import": "./dist/internal-testing-utils.js",
      "types": "./dist/internal-testing-utils.d.ts",
      "@nx/nx-source": "./internal-testing-utils.ts"
    },
    "./ngcli-adapter": {
      "import": "./dist/ngcli-adapter.js",
      "types": "./dist/ngcli-adapter.d.ts",
      "@nx/nx-source": "./ngcli-adapter.ts"
    },
    "./src/generators/*": {
      "import": "./dist/generators/*.js",
      "types": "./dist/generators/*.d.ts",
      "@nx/nx-source": "./src/generators/*.ts"
    },
    "./src/generators/plugin-migrations/*": {
      "import": "./dist/generators/plugin-migrations/*.js",
      "types": "./dist/generators/plugin-migrations/*.d.ts",
      "@nx/nx-source": "./src/generators/plugin-migrations/*.ts"
    },
    "./src/utils/*": {
      "import": "./dist/utils/*.js",
      "types": "./dist/utils/*.d.ts",
      "@nx/nx-source": "./src/utils/*.ts"
    },
    "./src/utils/async-iterable/*": {
      "import": "./dist/utils/async-iterable/*.js",
      "types": "./dist/utils/async-iterable/*.d.ts",
      "@nx/nx-source": "./src/utils/async-iterable/*.ts"
    },
    "./src/utils/catalog/*": {
      "import": "./dist/utils/catalog/*.js",
      "types": "./dist/utils/catalog/*.d.ts",
      "@nx/nx-source": "./src/utils/catalog/*.ts"
    }
  }
}
