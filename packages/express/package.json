{
  "name": "@nx/express",
  "version": "0.0.1",
  "private": false,
  "description": "The Nx Plugin for Express contains executors and generators for allowing your workspace to create powerful Express Node applications and APIs.",
  "repository": {
    "type": "git",
    "url": "https://github.com/nrwl/nx.git",
    "directory": "packages/express"
  },
  "keywords": [
    "Monorepo",
    "Node",
    "Express",
    "Jest",
    "Cypress",
    "CLI",
    "Backend"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "author": "Victor Savkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nrwl/nx/issues"
  },
  "homepage": "https://nx.dev",
  "generators": "./generators.json",
  "ng-update": {
    "requirements": {},
    "migrations": "./migrations.json"
  },
  "dependencies": {
    "@nx/devkit": "workspace:*",
    "@nx/js": "workspace:*",
    "@nx/node": "workspace:*",
    "tslib": "catalog:typescript"
  },
  "peerDependencies": {
    "express": "^4.21.2"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "@nx/nx-source": "./src/index.ts"
    },
    "./package.json": "./package.json",
    "./migrations.json": "./migrations.json",
    "./generators.json": "./generators.json",
    "./src/utils/*": {
      "import": "./dist/utils/*.js",
      "types": "./dist/utils/*.d.ts",
      "@nx/nx-source": "./src/utils/*.ts"
    }
  }
}
