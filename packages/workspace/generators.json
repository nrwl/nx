{
  "name": "nx/workspace",
  "version": "0.1",
  "generators": {
    "preset": {
      "factory": "./dist/src/generators/preset/preset#presetGenerator",
      "schema": "./dist/src/generators/preset/schema.json",
      "description": "Create application in an empty workspace.",
      "x-use-standalone-layout": true,
      "hidden": true
    },
    "move": {
      "factory": "./dist/src/generators/move/move",
      "schema": "./dist/src/generators/move/schema.json",
      "aliases": ["mv"],
      "description": "Move an application or library to another folder."
    },
    "remove": {
      "factory": "./dist/src/generators/remove/remove#removeGenerator",
      "schema": "./dist/src/generators/remove/schema.json",
      "aliases": ["rm"],
      "description": "Remove an application or library."
    },
    "convert-to-monorepo": {
      "factory": "./dist/src/generators/convert-to-monorepo/convert-to-monorepo",
      "schema": "./dist/src/generators/convert-to-monorepo/schema.json",
      "description": "Convert a Nx project to a monorepo."
    },
    "new": {
      "factory": "./dist/src/generators/new/new#newGenerator",
      "schema": "./dist/src/generators/new/schema.json",
      "description": "Create a workspace.",
      "hidden": true
    },
    "run-commands": {
      "factory": "./dist/src/generators/run-commands/run-commands#runCommandsGenerator",
      "schema": "./dist/src/generators/run-commands/schema.json",
      "aliases": ["run-command", "target"],
      "description": "Generates a target to run any command in the terminal."
    },
    "fix-configuration": {
      "factory": "./dist/src/generators/convert-to-nx-project/convert-to-nx-project#convertToNxProjectGenerator",
      "schema": "./dist/src/generators/convert-to-nx-project/schema.json",
      "aliases": ["convert-to-nx-project"],
      "description": "Fixes projects configuration"
    },
    "npm-package": {
      "factory": "./dist/src/generators/npm-package/npm-package#npmPackageGenerator",
      "schema": "./dist/src/generators/npm-package/schema.json",
      "description": "Create a minimal NPM package.",
      "x-type": "library"
    },
    "ci-workflow": {
      "factory": "./dist/src/generators/ci-workflow/ci-workflow#ciWorkflowGenerator",
      "schema": "./dist/src/generators/ci-workflow/schema.json",
      "description": "Generate a CI workflow."
    },
    "infer-targets": {
      "factory": "./dist/src/generators/infer-targets/infer-targets",
      "schema": "./dist/src/generators/infer-targets/schema.json",
      "description": "Convert Nx projects to use inferred targets."
    }
  }
}
