{
  "name": "@nx/docker",
  "description": "The Nx Plugin for Docker to aid in containerizing projects.",
  "version": "0.0.1",
  "type": "commonjs",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nrwl/nx.git",
    "directory": "packages/docker"
  },
  "bugs": {
    "url": "https://github.com/nrwl/nx/issues"
  },
  "keywords": [
    "Monorepo",
    "Docker",
    "Containers"
  ],
  "author": "Colum Ferry",
  "license": "MIT",
  "homepage": "https://nx.dev",
  "main": "index.js",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./release/version-actions": {
      "types": "./src/release/version-actions.d.ts",
      "default": "./src/release/version-actions.js"
    },
    "./package.json": {
      "default": "./package.json"
    }
  },
  "executors": "./executors.json",
  "dependencies": {
    "@nx/devkit": "file:../devkit",
    "tslib": "^2.3.0"
  }
}
