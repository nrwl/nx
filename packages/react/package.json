{
  "name": "@nx/react",
  "version": "0.0.1",
  "private": false,
  "description": "The React plugin for Nx contains executors and generators for managing React applications and libraries within an Nx workspace. It provides:\n\n\n- Integration with libraries such as Jest, Vitest, Playwright, Cypress, and Storybook.\n\n- Generators for applications, libraries, components, hooks, and more.\n\n- Library build support for publishing packages to npm or other registries.\n\n- Utilities for automatic workspace refactoring.",
  "repository": {
    "type": "git",
    "url": "https://github.com/nrwl/nx.git",
    "directory": "packages/react"
  },
  "keywords": [
    "Monorepo",
    "React",
    "Web",
    "Jest",
    "Cypress",
    "CLI",
    "Front-end"
  ],
  "main": "./index.js",
  "typings": "./index.d.ts",
  "author": "Victor Savkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nrwl/nx/issues"
  },
  "homepage": "https://nx.dev",
  "executors": "./executors.json",
  "generators": "./generators.json",
  "ng-update": {
    "requirements": {},
    "migrations": "./migrations.json"
  },
  "dependencies": {
    "@phenomnomnominal/tsquery": "catalog:typescript",
    "@svgr/webpack": "^8.0.1",
    "file-loader": "^6.2.0",
    "minimatch": "catalog:",
    "picocolors": "catalog:",
    "tslib": "catalog:typescript",
    "@nx/devkit": "workspace:*",
    "@nx/js": "workspace:*",
    "@nx/eslint": "workspace:*",
    "@nx/web": "workspace:*",
    "@nx/module-federation": "workspace:*",
    "@nx/rollup": "workspace:*",
    "express": "^4.21.2",
    "http-proxy-middleware": "^3.0.5",
    "semver": "catalog:"
  },
  "devDependencies": {
    "@nx/cypress": "workspace:*",
    "@nx/playwright": "workspace:*",
    "@nx/rsbuild": "workspace:*",
    "@nx/vite": "workspace:*",
    "@nx/vitest": "workspace:*",
    "@nx/webpack": "workspace:*",
    "nx": "workspace:*"
  },
  "optionalDependencies": {
    "@nx/vite": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "@nx/nx-source": "./src/index.ts"
    },
    "./package.json": "./package.json",
    "./migrations.json": "./migrations.json",
    "./generators.json": "./generators.json",
    "./executors.json": "./executors.json",
    "./plugins/*": {
      "import": "./plugins/*.js",
      "types": "./plugins/*.d.ts",
      "@nx/nx-source": "./plugins/*.ts"
    },
    "./src/utils/*": {
      "import": "./dist/utils/*.js",
      "types": "./dist/utils/*.d.ts",
      "@nx/nx-source": "./src/utils/*.ts"
    },
    "./src/generators/*": {
      "import": "./dist/generators/*.js",
      "types": "./dist/generators/*.d.ts",
      "@nx/nx-source": "./src/generators/*.ts"
    },
    "./src/generators/*/lib/*": {
      "import": "./dist/generators/*/lib/*.js",
      "types": "./dist/generators/*/lib/*.d.ts",
      "@nx/nx-source": "./src/generators/*/lib/*.ts"
    },
    "./src/generators/*/schema.json": "./src/generators/*/schema.json",
    "./src/executors/*": {
      "import": "./dist/executors/*.js",
      "types": "./dist/executors/*.d.ts",
      "@nx/nx-source": "./src/executors/*.ts"
    },
    "./src/executors/*/lib/*": {
      "import": "./dist/executors/*/lib/*.js",
      "types": "./dist/executors/*/lib/*.d.ts",
      "@nx/nx-source": "./src/executors/*/lib/*.ts"
    },
    "./src/executors/*/schema.json": "./src/executors/*/schema.json",
    "./src/module-federation/*": {
      "import": "./dist/module-federation/*.js",
      "types": "./dist/module-federation/*.d.ts",
      "@nx/nx-source": "./src/module-federation/*.ts"
    },
    "./mf": {
      "import": "./mf/index.js",
      "require": "./mf/index.js",
      "types": "./mf/index.d.ts",
      "@nx/nx-source": "./mf/index.ts"
    },
    "./babel": {
      "import": "./babel.js",
      "require": "./babel.js",
      "types": "./babel.d.ts",
      "@nx/nx-source": "./babel.ts"
    },
    "./tailwind": {
      "import": "./tailwind.js",
      "require": "./tailwind.js",
      "types": "./tailwind.d.ts",
      "@nx/nx-source": "./tailwind.ts"
    },
    "./module-federation": {
      "import": "./module-federation.js",
      "require": "./module-federation.js",
      "types": "./module-federation.d.ts",
      "@nx/nx-source": "./module-federation.ts"
    },
    "./webpack-plugin": {
      "import": "./webpack-plugin.js",
      "require": "./webpack-plugin.js",
      "types": "./webpack-plugin.d.ts",
      "@nx/nx-source": "./webpack-plugin.ts"
    },
    "./router-plugin": {
      "import": "./router-plugin.js",
      "require": "./router-plugin.js",
      "types": "./router-plugin.d.ts",
      "@nx/nx-source": "./router-plugin.ts"
    },
    "./ast-utils": {
      "import": "./ast-utils.js",
      "require": "./ast-utils.js",
      "types": "./ast-utils.d.ts",
      "@nx/nx-source": "./ast-utils.ts"
    },
    "./plugins/storybook": {
      "import": "./plugins/storybook/index.js",
      "require": "./plugins/storybook/index.js",
      "types": "./plugins/storybook/index.d.ts",
      "@nx/nx-source": "./plugins/storybook/index.ts"
    },
    "./plugins/component-testing": {
      "import": "./plugins/component-testing/index.js",
      "require": "./plugins/component-testing/index.js",
      "types": "./plugins/component-testing/index.d.ts",
      "@nx/nx-source": "./plugins/component-testing/index.ts"
    },
    "./plugins/webpack": {
      "import": "./plugins/webpack.js",
      "require": "./plugins/webpack.js",
      "types": "./plugins/webpack.d.ts",
      "@nx/nx-source": "./plugins/webpack.ts"
    },
    "./plugins/bundle-rollup": {
      "import": "./plugins/bundle-rollup.js",
      "require": "./plugins/bundle-rollup.js",
      "types": "./plugins/bundle-rollup.d.ts",
      "@nx/nx-source": "./plugins/bundle-rollup.ts"
    },
    "./plugins/jest": {
      "import": "./plugins/jest.js",
      "require": "./plugins/jest.js",
      "types": "./plugins/jest.d.ts",
      "@nx/nx-source": "./plugins/jest.ts"
    },
    "./plugins/with-react": {
      "import": "./plugins/with-react.js",
      "require": "./plugins/with-react.js",
      "types": "./plugins/with-react.d.ts",
      "@nx/nx-source": "./plugins/with-react.ts"
    },
    "./src/utils": {
      "import": "./src/utils/index.js",
      "require": "./src/utils/index.js",
      "types": "./src/utils/index.d.ts",
      "@nx/nx-source": "./src/utils/index.ts"
    },
    "./src/generators/utils": {
      "import": "./src/generators/utils/index.js",
      "require": "./src/generators/utils/index.js",
      "types": "./src/generators/utils/index.d.ts",
      "@nx/nx-source": "./src/generators/utils/index.ts"
    },
    "./src/generators/*/*": {
      "import": "./src/generators/*/*.js",
      "require": "./src/generators/*/*.js",
      "types": "./src/generators/*/*.d.ts",
      "@nx/nx-source": "./src/generators/*/*.ts"
    },
    "./src/executors/*.impl": {
      "import": "./src/executors/*.impl.js",
      "require": "./src/executors/*.impl.js",
      "types": "./src/executors/*.impl.d.ts",
      "@nx/nx-source": "./src/executors/*.impl.ts"
    },
    "./src/plugins/*": {
      "import": "./src/plugins/*.js",
      "require": "./src/plugins/*.js",
      "types": "./src/plugins/*.d.ts",
      "@nx/nx-source": "./src/plugins/*.ts"
    },
    "./src/rules/*": {
      "import": "./src/rules/*.js",
      "require": "./src/rules/*.js",
      "types": "./src/rules/*.d.ts",
      "@nx/nx-source": "./src/rules/*.ts"
    },
    "./src/*": {
      "import": "./src/*.js",
      "require": "./src/*.js",
      "types": "./src/*.d.ts",
      "@nx/nx-source": "./src/*.ts"
    },
    "./mf/*": {
      "import": "./mf/*.js",
      "require": "./mf/*.js",
      "types": "./mf/*.d.ts",
      "@nx/nx-source": "./mf/*.ts"
    },
    "./typings/*.d.ts": "./typings/*.d.ts"
  }
}
