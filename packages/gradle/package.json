{
  "name": "@nx/gradle",
  "version": "0.0.1",
  "private": false,
  "description": "The Nx Plugin for Gradle allows Gradle tasks to be run through Nx",
  "repository": {
    "type": "git",
    "url": "https://github.com/nrwl/nx.git",
    "directory": "packages/gradle"
  },
  "keywords": [
    "Monorepo",
    "Java",
    "Gradle",
    "CLI",
    "Backend"
  ],
  "main": "./index.js",
  "typings": "./index.d.ts",
  "author": "Victor Savkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nrwl/nx/issues"
  },
  "homepage": "https://nx.dev",
  "generators": "./generators.json",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "@nx/nx-source": "./src/index.ts"
    },
    "./package.json": "./package.json",
    "./migrations.json": "./migrations.json",
    "./generators.json": "./generators.json",
    "./executors.json": "./executors.json",
    "./src/utils/*": {
      "import": "./dist/utils/*.js",
      "types": "./dist/utils/*.d.ts",
      "@nx/nx-source": "./src/utils/*.ts"
    },
    "./src/generators/*": {
      "import": "./dist/generators/*.js",
      "types": "./dist/generators/*.d.ts",
      "@nx/nx-source": "./src/generators/*.ts"
    },
    "./src/executors/*": {
      "import": "./dist/executors/*.js",
      "types": "./dist/executors/*.d.ts",
      "@nx/nx-source": "./src/executors/*.ts"
    },
    "./plugins/*": {
      "import": "./plugins/*.js",
      "types": "./plugins/*.d.ts",
      "@nx/nx-source": "./plugins/*.ts"
    },
    "./plugin-v1": "./plugin-v1.js"
  },
  "nx-migrations": {
    "migrations": "./migrations.json"
  },
  "dependencies": {
    "@nx/devkit": "workspace:*",
    "toml-eslint-parser": "^0.10.0"
  },
  "devDependencies": {
    "nx": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  },
  "executors": "./executors.json",
  "type": "commonjs"
}
