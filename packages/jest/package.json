{
  "name": "@nx/jest",
  "version": "0.0.1",
  "private": false,
  "type": "commonjs",
  "description": "The Nx Plugin for Jest contains executors and generators allowing your workspace to use the powerful Jest testing capabilities.",
  "repository": {
    "type": "git",
    "url": "https://github.com/nrwl/nx.git",
    "directory": "packages/jest"
  },
  "keywords": [
    "Monorepo",
    "Angular",
    "React",
    "Web",
    "Node",
    "Nest",
    "Jest",
    "Unit Testing",
    "CLI",
    "Testing"
  ],
  "main": "./dist/index.js",
  "typings": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./index.d.ts",
      "@nx/nx-source": "./index.ts"
    },
    "./plugin": {
      "import": "./dist/plugin.js",
      "types": "./plugin.d.ts",
      "@nx/nx-source": "./plugin.ts"
    },
    "./preset": {
      "import": "./dist/preset.js",
      "types": "./preset.d.ts",
      "@nx/nx-source": "./preset.ts"
    },
    "./preset/*": {
      "import": "./dist/preset/*.js",
      "types": "./preset/*.d.ts",
      "@nx/nx-source": "./preset/*.ts"
    },
    "./plugins/*": {
      "import": "./dist/plugins/*.js",
      "types": "./plugins/*.d.ts",
      "@nx/nx-source": "./plugins/*.ts"
    },
    "./src/generators/*": {
      "import": "./dist/src/generators/*.js",
      "types": "./src/generators/*.d.ts",
      "@nx/nx-source": "./src/generators/*.ts"
    },
    "./src/generators/**/*": {
      "import": "./dist/src/generators/**/*.js",
      "types": "./src/generators/**/*.d.ts",
      "@nx/nx-source": "./src/generators/**/*.ts"
    },
    "./src/executors/*": {
      "import": "./dist/src/executors/*.js",
      "types": "./src/executors/*.d.ts",
      "@nx/nx-source": "./src/executors/*.ts"
    },
    "./src/executors/**/*": {
      "import": "./dist/src/executors/**/*.js",
      "types": "./src/executors/**/*.d.ts",
      "@nx/nx-source": "./src/executors/**/*.ts"
    },
    "./src/utils/*": {
      "import": "./dist/src/utils/*.js",
      "types": "./src/utils/*.d.ts",
      "@nx/nx-source": "./src/utils/*.ts"
    },
    "./src/utils/**/*": {
      "import": "./dist/src/utils/**/*.js",
      "types": "./src/utils/**/*.d.ts",
      "@nx/nx-source": "./src/utils/**/*.ts"
    },
    "./package.json": "./package.json",
    "./generators.json": "./generators.json",
    "./executors.json": "./executors.json",
    "./migrations.json": "./migrations.json"
  },
  "author": "Victor Savkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nrwl/nx/issues"
  },
  "homepage": "https://nx.dev",
  "generators": "./generators.json",
  "executors": "./executors.json",
  "ng-update": {
    "requirements": {},
    "migrations": "./migrations.json"
  },
  "dependencies": {
    "@jest/reporters": "catalog:jest",
    "@jest/test-result": "catalog:jest",
    "@nx/devkit": "workspace:*",
    "@nx/js": "workspace:*",
    "@phenomnomnominal/tsquery": "catalog:typescript",
    "identity-obj-proxy": "catalog:jest",
    "jest-config": "catalog:jest",
    "jest-resolve": "catalog:jest",
    "jest-util": "catalog:jest",
    "minimatch": "catalog:",
    "picocolors": "catalog:",
    "resolve.exports": "2.0.3",
    "semver": "catalog:",
    "tslib": "catalog:typescript",
    "yargs-parser": "catalog:"
  },
  "devDependencies": {
    "nx": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
