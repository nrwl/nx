{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../../dist/out-tsc/nx-dev/ui-animations",
    "types": [],
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "composite": true,
    "declaration": true,
    "skipLibCheck": true,
    "strict": false,
    "noImplicitAny": false,
    "jsx": "react-jsx"
  },
  "files": [
    "../../node_modules/@nx/react/typings/cssmodule.d.ts",
    "../../node_modules/@nx/react/typings/image.d.ts"
  ],
  "exclude": [
    "jest.config.ts",
    "**/*.spec.ts",
    "**/*.test.ts",
    "**/*.spec.tsx",
    "**/*.test.tsx",
    "**/*.spec.js",
    "**/*.test.js",
    "**/*.spec.jsx",
    "**/*.test.jsx"
  ],
  "include": ["**/*.js", "**/*.jsx", "**/*.ts", "**/*.tsx"],
  "references": [{ "path": "../ui-primitives/tsconfig.lib.json" }]
}
