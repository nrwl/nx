{
    "name": "fluff-hello-world",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "apps/fluff-hello-world/src",
    "tags": [],
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run cli -- --cwd apps/fluff-hello-world build"
            },
            "outputs": [
                "{projectRoot}/dist"
            ]
        },
        "serve": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run cli -- --cwd apps/fluff-hello-world serve"
            }
        }
    }
}
