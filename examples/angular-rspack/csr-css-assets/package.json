{
  "name": "examples-angular-rspack-csr-css-assets",
  "version": "0.0.1",
  "private": true,
  "scripts": {},
  "dependencies": {
    "@nx/angular-rspack": "workspace:*"
  },
  "devDependencies": {
    "@rspack/core": "1.5.0",
    "@rspack/cli": "1.5.0"
  },
  "nx": {
    "targets": {
      "serve-api": {
        "continuous": true,
        "command": "node ./src/api.mjs",
        "options": {
          "cwd": "examples/angular-rspack/csr-css-assets"
        }
      },
      "serve": {
        "dependsOn": [
          "^build",
          "serve-api"
        ]
      },
      "build": {
        "dependsOn": [
          "^build"
        ]
      }
    }
  }
}
